    "release": "pnpm build --filter=\"./packages/*\" && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@fellipeutaka/eslint-config": "^1.8.1",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "turbo": "latest",
    "typescript": "^5.2.2"
  }
}
