{
  "name": "@fellipeutaka/ui",
  "version": "1.0.0",
  "description": "An accessible and customizable component library powered by shadcn/ui",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fellipeutaka/ui",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/fellipeutaka/ui/issues"
  },
  "homepage": "https://fellipeutaka-ui.vercel.app",
  "files": [
    "dist/**"
  ],
  "exports": {
    "./button": {
      "import": "./dist/button.mjs",
      "require": "./dist/button.js",
      "types": "./dist/button.d.ts"
    },
    "./dialog": {
      "import": "./dist/dialog.mjs",
      "require": "./dist/dialog.js",
      "types": "./dist/dialog.d.ts"
    },
    "./toast": {
      "import": "./dist/toast.mjs",
      "require": "./dist/toast.js",
      "types": "./dist/toast.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "clean": "rimraf .turbo node_modules dist"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "sonner": "^1.0.3"
  },
  "devDependencies": {
    "@fellipeutaka/styles": "workspace:*",
    "@fellipeutaka/tsconfig": "workspace:*",
    "@types/react": "^18.2.28",
    "lucide-react": "^0.288.0",
    "react": "^18.2.0",
    "tailwind-variants": "^0.1.14",
    "tsup": "^7.2.0"
  },
  "peerDependencies": {
    "@fellipeutaka/styles": "*",
    "react": "^18",
    "lucide-react": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
